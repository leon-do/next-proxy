{
  "rewrites": [
    {
      "source": "/api/graphql",
      "destination": "https://api.uniswap.org/v1/graphql"
    }
  ],
  "headers": [
    {
      "source": "/api/graphql",
      "headers": [{ "key": "Origin", "value": "http://localhost:3000" }]
    }
  ],
  "routes": [
    {
      "src": "/api/graphql",
      "headers": { "Origin": "http://localhost:3000" },
      "continue": true
    }
  ]
}
